<?xml version="1.0"?>
<!--
    ACL Configuration for Customer Feedback
    Author: Devall
    Website: https://developers-alliance.com
    Package: Devall_CustomerFeedback
    Version: 1.0.0

    This file defines the access control list (ACL) for the Customer Feedback module in Magento Admin.
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Devall_CustomerFeedback::feedback_main" title="Customer Feedback" sortOrder="10">
                    <resource id="Devall_CustomerFeedback::feedback_view" title="Manage Feedback" sortOrder="10"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
